    "test": "jest --forceExit",
    "check": "npm-check --update --skip-unused",
    "prepush": "node scripts/plugin.js",
    "compile": "node scripts/compile.js",
    "deploy": "node scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:anthonyli/heyouman.git"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.js": [
      "eslint --fix --format codeframe",
